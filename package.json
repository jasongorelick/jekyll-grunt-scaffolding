{
  "name": "jekyll-grunt-scaffold",
  "description": "replaces jekyll asset generation with grunt alternatives",
  "version": "0.1.1",
  "engines": {
    "node": ">=4.1.1"
  },
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-autoprefixer": "^1.0.0",
    "grunt-cache-bust": "^0.4.4",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-compass": "^0.9.1",
    "grunt-contrib-concat": "^0.5.0",
    "grunt-contrib-jshint": "^0.10.0",
    "grunt-contrib-nodeunit": "~0.4.1",
    "grunt-contrib-uglify": "^0.5.1",
    "grunt-contrib-watch": "^0.6.1",
    "jshint-stylish": "^0.4.0"
  },
  "main": "index.html",
  "dependencies": {},
  "config": {
    "jekyll": "_config.yml"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "jekyll serve --config $npm_package_config_jekyll & grunt watch:css & grunt watch:js",
    "build": "grunt prepare && jekyll build --config $npm_package_config_jekyll"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jqgorelick/jekyll-grunt-scaffolding.git"
  },
  "author": "jqgorelick",
  "license": "ISC"
}
